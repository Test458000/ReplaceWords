{
	"manifest_version": 2,
	"name": "ReWords",
	"description": "Replace some words in a web page using an inner dictionary",
	"version": "2.1",
	"icons": {"64": "icon-64.png"},
	"permissions": [
          "storage",
		  "tabs"
        ],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
		},
  	"content_scripts": [
		{
			"matches": [ "<all_urls>" ],
			"js": ["script.js"],
			"run_at": "document_end"
		}
	]
}